{
  "homepage": "http://programmingbymaleek.github.io/wimatechstore",
  "name": "e-commerce-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@headlessui/react": "^2.1.0",
    "@reduxjs/toolkit": "^2.2.5",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "firebase": "^10.12.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-redux": "^9.1.2",
    "react-router-dom": "^6.23.1",
    "react-scripts": "5.0.1",
    "sass": "^1.77.4",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "build:css": "postcss src/index.css -o src/styles/main.css",
    "watch:css": "postcss src/index.css -o src/styles.main.css -w",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "start": "npm-run-all -p watch:css start:react",
    "start:react": "react-scripts start",
    "build": "npm run build:css && react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "autoprefixer": "^10.4.19",
    "gh-pages": "^6.1.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.38",
    "postcss-cli": "^11.0.0",
    "tailwind": "^4.0.0",
    "tailwindcss": "^3.4.4"
  }
}
